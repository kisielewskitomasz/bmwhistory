                          <xs:extension base="xs:string">
                            <xs:attribute name="wojewodztwo" type="xs:string" use="required" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="kod" type="xs:string" />
                    <xs:element name="miasto" type="xs:string" />
