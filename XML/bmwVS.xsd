                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="data" type="xs:date" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element maxOccurs="unbounded" name="dostawa">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="podzespol">
